<h2><%= trn_get('TITLE_OPENING_EDIT') %></h2>

<%= form_tag door_frame_path(@door_frame), :method => :put do %>
  <%= render :partial => 'form' %>
  <%= submit_tag trn_geth('BUTTON_EDIT') %>
<% end %>
<%= link_to trn_geth('BUTTON_BACK_TO_LIST'), door_frame_index_path %> |
<%= link_to trn_geth('BUTTON_DELETE'), @door_frame, :confirm => trn_geth('LABEL_DELETE_QUESTION'), :method => :delete %>
